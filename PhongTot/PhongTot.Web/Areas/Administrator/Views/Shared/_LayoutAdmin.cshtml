<!DOCTYPE html>
<html ng-app="myAppAdmin" lang="en">
<head>
    <meta charset="utf-8">
    <title></title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-touch-fullscreen" content="yes">
    <meta name="description" content="Avenger Admin Theme">
    <meta name="author" content="KaijuThemes">
    <link href='http://fonts.googleapis.com/css?family=RobotoDraft:300,400,400italic,500,700' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300,400,400italic,600,700' rel='stylesheet' type='text/css'>
    <link type="text/css" href="~/Content/Admin/fonts/font-awesome/css/font-awesome.min.css" rel="stylesheet">     <!-- Font Awesome -->
    <link href="~/bower_components/toastr/toastr.css" rel="stylesheet" />
    <link type="text/css" href="~/Content/Admin/css/styles.css" rel="stylesheet">                                     <!-- Core CSS with all styles -->
</head>
<body class="infobar-offcanvas">
    <header id="topnav" class="navbar navbar-midnightblue navbar-fixed-top clearfix" role="banner">
        @RenderPage("_HeaderAdmin.cshtml")
    </header>
    <div id="wrapper">
        <div id="layout-static">
            @RenderPage("_SidebarMenuAdmin.cshtml")
            <div class="static-content-wrapper">
                <div class="static-content">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <!-- Load site level scripts -->
    <script type="text/javascript" src="~/Content/Admin/js/jquery-1.10.2.min.js"></script> 							<!-- Load jQuery -->
    <script type="text/javascript" src="~/Content/Admin/js/bootstrap.min.js"></script> 								<!-- Load Bootstrap -->
				<!-- Code Prettifier  -->
    <script type="text/javascript" src="~/Content/Admin/js/enquire.min.js"></script> 									<!-- Enquire for Responsiveness -->						<!-- Bootbox -->
    <script type="text/javascript" src="~/Content/Admin/plugins/nanoScroller/js/jquery.nanoscroller.min.js"></script> <!-- nano scroller -->
    <script type="text/javascript" src="~/Content/Admin/js/application.js"></script>
    <!-- End loading site level scripts -->
    <!-- Angularjs -->
    <script src="~/bower_components/angular/angular.js"></script>
    <script src="~/Areas/Administrator/app/app.js"></script>
    <script src="~/bower_components/toastr/toastr.js"></script>
    <script src="~/Areas/Administrator/app/commonService/notificationService.js"></script>
    <script src="~/Areas/Administrator/app/services/apiService.js"></script>
    <script src="~/bower_components/angular-sanitize/angular-sanitize.js"></script>
    <script src="~/bower_components/ng-timeago/ngtimeago.js"></script>
    <script src="~/Areas/Administrator/app/directives/pagerDirective.js"></script>
    <script src="~/Areas/Administrator/app/controllers/InfoController/infoAdminController.js"></script>
    <script src="~/Areas/Administrator/app/controllers/PostController/postAdminController.js"></script>
</body>
</html>